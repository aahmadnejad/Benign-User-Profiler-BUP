        {
          "name": "ping_scan",
          "type": "CMD",
          "commands": [
            {
              "command": "ping -c 3 192.168.1.1",
              "platform": "linux",
              "show_output": true,
              "wait_after": 2
            },
            {
              "command": "for i in {1..10}; do ping -c 1 -W 1 192.168.1.$i; done",
              "platform": "linux",
              "show_output": true,
              "wait_after": 5
            }
          ],
          "windows_commands": [
            {
              "command": "ping 192.168.1.1",
              "platform": "windows",
              "show_output": true,
              "wait_after": 2
            },
            {
              "command": "for /L %i in (1,1,10) do @ping -n 1 -w 1000 192.168.1.%i",
              "platform": "windows",
              "show_output": true,
              "wait_after": 5
            }
          ],
          "frequency": 1,
          "time_interval": [3600, 7200],
          "start_time": "11:00",
          "start_time_format": "%H:%M"
        }
